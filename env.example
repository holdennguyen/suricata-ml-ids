# ML Training Configuration
ML_ACCURACY_TARGET=0.90

# Service Configuration
SURICATA_LOG_LEVEL=info
OPENSEARCH_JAVA_OPTS=-Xms512m -Xmx512m

# Development Mode (set to true for development)
DEV_MODE=true
